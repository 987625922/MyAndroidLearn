<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wyt.zdf.myapplication">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 获取联系人 -->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name="com.wind.androidlearn.APP"
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:appComponentFactory">
        <activity android:name="com.wind.androidlearn.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--BroadcastReceiver广播实现跨进程数据交互-->
        <service android:name="com.wind.androidlearn.bassis.broadcast.MsgService"></service>
        <activity android:name="com.wind.androidlearn.bassis.broadcast.MainActivity" />

        <!--binder跨进程数据交互-->
        <service android:name="com.wind.androidlearn.bassis.broadcast.binder.MsgService"></service>
        <activity android:name="com.wind.androidlearn.bassis.broadcast.binder.MainActivity" />

        <!--content provider实现-->
        <provider
            android:name="com.wind.androidlearn.bassis.provider.MyProvider"
            android:authorities="cn.scu.myprovider" />
        <activity
            android:name="com.wind.androidlearn.bassis.provider.ProviderActivity"
            tools:ignore="DuplicateActivity" />

        <!--RecyclerView加载多图-->
        <activity android:name="com.wind.androidlearn.bassis.RecyclerImage.RecycleActivity"></activity>
        <!-- retrofit -->
        <activity android:name="com.wind.androidlearn.okhttp.retrofit.GetRequestActivity"></activity>
        <!-- bitmap -->
        <activity android:name="com.wind.androidlearn.img.BitmapActivity"></activity>
        <!-- 头条适配方案 -->
        <activity android:name="com.wind.androidlearn.bassis.Utils.screenmatch.DensityActivity" />
        <!-- fragment使用 -->
        <activity android:name="com.wind.androidlearn.bassis.fragment.FragmentActivity" />
        <!-- b站视频播放 -->
        <activity android:name="com.wind.androidlearn.ijkplayer.IjkplayActivity" />
        <activity android:name="com.wind.androidlearn.ijkplayer.VideoPlayerActivity" />
        <!-- viewpager实现广告页 -->
        <activity android:name="com.wind.androidlearn.bassis.viewpager.ViewpagerActivity" />
        <!-- butter使用 -->
        <activity android:name="com.wind.androidlearn.butterknifeuse.ButterActivity" />
        <!-- room的使用 -->
        <activity android:name="com.wind.androidlearn.room.RoomActivity" />
        <!-- bus使用 -->
        <activity android:name="com.wind.androidlearn.eventbusandlivedatabus.BusActivity" />
        <!-- Android Architecture Components的使用 -->
        <activity android:name="com.wind.androidlearn.aac.ui.UserActivity" />
        <!-- 获取联系人 -->
        <activity android:name="com.wind.androidlearn.bassis.获取联系人" />
        <!-- ARouter的使用-->
        <activity android:name="com.wind.androidlearn.arouter.ARouterUseActivity" />
        <!-- livedata -->
        <activity android:name="com.wind.androidlearn.livedata.NameActivity" />
        <!-- kotlin使用 -->
        <activity android:name="com.wind.androidlearn.kotlin.MainActivity" />
        <!-- 设置模式 -->
        <activity android:name="com.wind.androidlearn.设计模式.PatternActivity" />

        <provider
            android:name="com.wind.androidlearn.bassis.provider.MyProvider"
            android:authorities="cn.scu.myprovider" />
    </application>

</manifest>