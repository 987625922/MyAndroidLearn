<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wyt.zdf.myapplication">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 获取联系人 -->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name="com.wind.androidlearn.APP"
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:appComponentFactory">
        <activity
            android:name="com.wind.androidlearn.MainActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--BroadcastReceiver广播实现跨进程数据交互-->
        <service android:name="com.wind.androidlearn.bassis.broadcast.MsgService"></service>
        <activity android:name="com.wind.androidlearn.bassis.broadcast.MainActivity" />

        <!--binder跨进程数据交互-->
        <service android:name="com.wind.androidlearn.bassis.broadcast.binder.MsgService"></service>
        <activity android:name="com.wind.androidlearn.bassis.broadcast.binder.MainActivity" />

        <!--content provider实现-->
        <provider
            android:name="com.wind.androidlearn.bassis.provider.MyProvider"
            android:authorities="cn.scu.myprovider" />
        <activity
            android:name="com.wind.androidlearn.bassis.provider.ProviderActivity"
            tools:ignore="DuplicateActivity" />
        <!-- 头条适配方案 -->
        <activity android:name="com.wind.androidlearn.bassis.Utils.screenmatch.DensityActivity">
            <!-- 隐式调用 -->
            <!--   scheme://host:port/path|pathPrefix|pathPattern         -->
            <!--       Intent intent = new Intent();
                    intent.setAction("com.learn.action.enter");
                    intent.setData(Uri.parse("learn1://learn.com:8888/first?message=Hello FirstActivity"));
                    startActivity(intent);       -->
            <intent-filter>
                <action android:name="com.learn.action.enter" />
                <action android:name="com.learn.action.enter1" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.ALTERNATIVE" />
                <data
                    android:host="learn.com"
                    android:path="/first"
                    android:port="8888"
                    android:scheme="learn1" />
            </intent-filter>
        </activity>
        <!-- fragment使用 -->
        <activity android:name="com.wind.androidlearn.bassis.fragment.FragmentActivity" />
        <!-- viewpager实现广告页 -->
        <activity android:name="com.wind.androidlearn.bassis.viewpager.ViewpagerActivity" />
        <!-- room的使用 -->
        <activity android:name="com.wind.androidlearn.room.RoomActivity" />
        <!-- 获取联系人 -->
        <!-- kotlin使用 -->
        <activity android:name="com.wind.androidlearn.kotlin.MainActivity" />
        <!-- 设置模式 -->
        <activity android:name="com.wind.androidlearn.设计模式.PatternActivity" />

        <provider
            android:name="com.wind.androidlearn.bassis.provider.MyProvider"
            android:authorities="cn.scu.myprovider" />
    </application>

</manifest>